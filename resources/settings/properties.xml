<resources>

<properties>
	<property id="top_complication" type="number">0</property>
	<property id="bot_complication" type="number">1</property>
	<property id="complication_location" type="number">0</property>
	<property id="top_numeric" type="number">0</property>
	<property id="secondshand" type="boolean">true</property>
</properties>

<strings>
	<string id="complication_title_top">Top field</string>
	<string id="complication_title_bot">Bottom field</string>
	<string id="complication_title_location">Datafield location</string>
	<string id="date">Date</string>
	<string id="battery">Battery %</string>
	<string id="heartrate">Heartrate</string>
	<string id="steps">Today's steps</string>
	<string id="blank">Blank</string>
	<string id="rotate">Rotate with hands</string>
	<string id="twelve">12 o'clock</string>
	<string id="three">3 o'clock</string>
	<string id="six">6 o'clock</string>
	<string id="nine">9 o'clock</string>
	<string id="top_symbol_numeric">Numeric 12</string>
	<string id="top_symbol_triangle">Flieger-style triangle</string>
	<string id="top_symbol_title">12 o'clock indicator</string>
	<string id="seconds_title">Enable or disable seconds hand</string>
</strings>

<settings>
	<setting propertyKey="@Properties.top_numeric" title="@Strings.top_symbol_title">
		<settingConfig type="list">
			<listEntry value="0">@Strings.top_symbol_triangle</listEntry>
			<listEntry value="1">@Strings.top_symbol_numeric</listEntry>
		</settingConfig>
	</setting>

	<setting propertyKey="@Properties.secondshand" title="@Strings.seconds_title">
		<settingConfig type="boolean" />
	</setting>

	<setting propertyKey="@Properties.complication_location" title="@Strings.complication_title_location">
		<settingConfig type="list">
			<listEntry value="0">@Strings.rotate</listEntry>
			<listEntry value="1">@Strings.twelve</listEntry>
			<listEntry value="2">@Strings.three</listEntry>
			<listEntry value="3">@Strings.six</listEntry>
			<listEntry value="4">@Strings.nine</listEntry>
		</settingConfig>
	</setting>

	<setting propertyKey="@Properties.top_complication" title="@Strings.complication_title_top">
		<settingConfig type="list">
			<listEntry value="0">@Strings.date</listEntry>
			<listEntry value="1">@Strings.battery</listEntry>
			<listEntry value="2">@Strings.heartrate</listEntry>
			<listEntry value="3">@Strings.steps</listEntry>
			<listEntry value="4">@Strings.blank</listEntry>
		</settingConfig>
	</setting>
	
	<setting propertyKey="@Properties.bot_complication" title="@Strings.complication_title_bot">
		<settingConfig type="list">
			<listEntry value="0">@Strings.date</listEntry>
			<listEntry value="1">@Strings.battery</listEntry>
			<listEntry value="2">@Strings.heartrate</listEntry>
			<listEntry value="3">@Strings.steps</listEntry>
			<listEntry value="4">@Strings.blank</listEntry>
		</settingConfig>
	</setting>
</settings>

</resources>